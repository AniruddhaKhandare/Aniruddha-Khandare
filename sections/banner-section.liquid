<!-- Banner Section -->
<div class="banner-section" id="banner-{{ section.id }}">
  <div class="banner-container">
    <!-- Header with Brand -->
    <div class="banner-header">
      <div class="hamburger-menu">
        <span></span>
        <span></span>
        <span></span>
      </div>
      <h1 class="brand-name">{{ section.settings.brand_name }}</h1>
    </div>

    <!-- Main Content -->
    <div class="banner-content">
      <div class="banner-text">
        <h2 class="banner-headline">{{ section.settings.headline }}</h2>
        <p class="banner-subtitle">{{ section.settings.subtitle }}</p>
        {% if section.settings.description != blank %}
          <p class="banner-description">{{ section.settings.description }}</p>
        {% endif %}
        
        <!-- CTA Button -->
        <div class="banner-cta">
          <a href="{{ section.settings.button_link | default: '/collections/all' }}" class="shop-now-btn">
            {{ section.settings.button_text }}
            <svg width="20" height="16" viewBox="0 0 20 16" fill="none">
              <path d="M12 1L19 8L12 15M19 8H1" stroke="currentColor" stroke-width="2"/>
            </svg>
          </a>
        </div>
      </div>

      <!-- Illustration -->
      <div class="banner-illustration">
        <svg class="gift-illustration" viewBox="0 0 400 300" fill="none">
          <!-- Camera -->
          <rect x="260" y="50" width="80" height="60" rx="8" stroke="currentColor" stroke-width="2"/>
          <circle cx="300" cy="80" r="20" stroke="currentColor" stroke-width="2"/>
          <circle cx="300" cy="80" r="12" stroke="currentColor" stroke-width="2"/>
          <rect x="320" y="65" width="15" height="8" stroke="currentColor" stroke-width="2"/>
          
          <!-- Lens -->
          <circle cx="120" cy="90" r="35" stroke="currentColor" stroke-width="2"/>
          <rect x="90" y="125" width="60" height="20" stroke="currentColor" stroke-width="2"/>
          
          <!-- Watch -->
          <circle cx="350" cy="180" r="25" stroke="currentColor" stroke-width="2"/>
          <line x1="350" y1="165" x2="350" y2="180" stroke="currentColor" stroke-width="2"/>
          <line x1="350" y1="180" x2="360" y2="190" stroke="currentColor" stroke-width="2"/>
          <rect x="320" y="190" width="60" height="15" rx="7" stroke="currentColor" stroke-width="2"/>
          
          <!-- Map -->
          <rect x="50" y="180" width="100" height="80" stroke="currentColor" stroke-width="2"/>
          <path d="M60 200 Q80 190 100 200 Q120 210 140 200" stroke="currentColor" stroke-width="2"/>
          <circle cx="90" cy="220" r="3" fill="currentColor"/>
          
          <!-- Pen -->
          <line x1="370" y1="120" x2="390" y2="140" stroke="currentColor" stroke-width="3"/>
          <circle cx="368" cy="118" r="3" stroke="currentColor" stroke-width="2"/>
          
          <!-- Notebook -->
          <rect x="180" y="160" width="60" height="80" stroke="currentColor" stroke-width="2"/>
          <line x1="190" y1="180" x2="230" y2="180" stroke="currentColor" stroke-width="1"/>
          <line x1="190" y1="200" x2="230" y2="200" stroke="currentColor" stroke-width="1"/>
          <line x1="190" y1="220" x2="220" y2="220" stroke="currentColor" stroke-width="1"/>
        </svg>
      </div>
    </div>

    <!-- Footer Text -->
    <div class="banner-footer">
      <p class="sustainable-text">{{ section.settings.footer_text }}</p>
    </div>
  </div>
</div>

